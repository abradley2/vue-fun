{
  "name": "vue-fun",
  "version": "1.0.0",
  "main": "index.js",
  "repository": {},
  "test": "standard",
  "license": "MIT",
  "scripts": {
    "start-dev": "NODE_ENV=development concurrently \"node server\" \"budo client/main.js:main.bundle.js --live --pushstate --dir public\"",
    "start-prod": "NODE_ENV=production browserify client/main.js -o ./public/main.bundle.js && node server"
  },
  "browserify": {
    "transform": [
      "vueify",
      "envify"
    ]
  },
  "vueify": {
    "postcss": [
      "postcss-cssnext",
      "autoprefixer"
    ]
  },
  "dependencies": {
    "express": "^4.14.1",
    "json-git": "^0.1.0",
    "level": "^1.6.0",
    "lodash": "^4.17.4",
    "memdb": "^1.3.1",
    "request": "^2.79.0",
    "vue": "^2.1.10",
    "vue-router": "^2.2.0",
    "vuex": "^2.1.2",
    "xhr": "^2.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "browserify": "^14.0.0",
    "budo": "^9.4.7",
    "concurrently": "^3.3.0",
    "envify": "^4.0.0",
    "postcss": "^5.2.12",
    "postcss-cssnext": "^2.9.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "uglifyify": "^3.0.4",
    "vueify": "^9.4.0"
  }
}
